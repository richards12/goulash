@PACKAGE_INIT@

set(IONIAN_INSTALL_PREFIX @CMAKE_INSTALL_PREFIX@ CACHE PATH "Ionian Installation Prefix")
set(IONIAN_USE_RDC @IONIAN_USE_RDC@ CACHE BOOL "Ionian RDC")

add_library(ionian STATIC IMPORTED)
find_library(IONIAN_LIB_PATH ionian PATHS "${CMAKE_CURRENT_LIST_DIR}/../..")
